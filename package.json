{
  "name": "wordpressgraphqlclient",
  "version": "1.0.1",
  "description": "utility for fetching and managing WordPress data using GraphQL.",
  "type": "module",
  "exports": {
          "./*": {
              "import": "./dist/esm/*.js",
              "require": "./dist/cjs/*.js",
              "node": "./src/*.ts"
          },
          "./dist/*": {
              "import": "./dist/esm/*",
              "require": "./dist/cjs/*"
          },
          "./src/*": "./src/*.ts"
    },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmdevapp/WordpressGraphQLClient.git"
  },
  "author": "Martin Michel <martin@mmdev.app>",
  "license": "SEE LICENSE IN <filename>",
  "bugs": {
    "url": "https://github.com/mmdevapp/WordpressGraphQLClient/issues"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8"
  },
  "homepage": "https://github.com/mmdevapp/WordpressGraphQLClient#readme"
}
